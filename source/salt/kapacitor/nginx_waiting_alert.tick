dbrp "telegraf"."autogen" 
    stream 
    // Select nginx measurement from fastcampus influxDB database.
        |from() 
            .measurement('nginx') 
        |alert()
            .id('{{ index .Tags "server"}}/waiting')
            .crit(lambda: int("waiting") > 0) 
        // Send an alert to Fastcampus Slack. 
        .slack()
